{
  "name": "asset-price-predict",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.0.0",
  "description": "Web-app for forecasting timeseries of asset prices",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "start": "pnpm -r start",
    "api:dev": "pnpm --filter @assetpredict/api dev",
    "api:build": "pnpm --filter @assetpredict/api build",
    "api:start": "pnpm --filter @assetpredict/api start",
    "lint": "eslint . && prettier --check . && stylelint \"**/*.{css,scss}\"",
    "verify:model": "node scripts/check_model_artifacts.mjs",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test",
    "test-cov": "pnpm -r test-cov",
    "coverage:report": "pnpm -r coverage:report",
    "e2e": "pnpm -r e2e"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sidimekov/asset-price-predict.git"
  },
  "private": true
}

{
  "name": "@assetpredict/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "node --test --import tsx ./test/env.test.ts",
    "e2e": "node --test --import tsx ./test/**.e2e.test.ts",
    "lint": "eslint . && prettier --check .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@assetpredict/shared": "workspace:*",
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.3.1",
    "@fastify/static": "^7.0.4",
    "fastify": "^4.28.1",
    "pg": "^8.13.1",
    "pino": "^9.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "@types/pg": "^8.16.0",
    "pino-pretty": "^11.3.0",
    "tsx": "^4.19.2"
  }
}
